<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:ns1="http://cse.campussource.de/lsf/wsdl/LSFSynchronizationMessage" xmlns:ns2="http://cse.campussource.de/lsf/bpel/SyncMsgTransformation" xmlns:ns3="http://cse.campussource.de/cse/wsdl/Course" xmlns:ns4="http://cse.campussource/cse/bpel/CreateCourse" xmlns:ns5="http://cse.campussource.de/lsf/wsdl/SynchronizationMessage" xmlns:ns6="http://enterprise.netbeans.org/bpel/CSEImprove/SplittSyncMessage" version="1.0">
    <service-assembly>
        <identification>
            <name>CSEImproveCA</name>
            <description>Represents the Service Assembly of CSEImproveCA</description>
        </identification>
        <connections>
            <connection>
