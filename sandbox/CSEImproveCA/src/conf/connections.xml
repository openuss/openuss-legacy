<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:ns1="CSEImproveCA" xmlns:ns2="http://cse.campussource.de/lsf/bpel/SyncMsgTransformation" xmlns:ns3="http://cse.campussource.de/lsf/wsdl/LSFSynchronizationMessage" xmlns:ns4="http://cse.campussource.de/cse/wsdl/CourseWrapper" xmlns:ns5="http://cse.campussource/cse/bpel/CreateCourse" xmlns:ns6="http://cse.campussource.de/lsf/wsdl/SynchronizationMessage" xmlns:ns7="http://enterprise.netbeans.org/bpel/CSEImprove/SplittSyncMessage" version="1.0">
    <service-assembly>
        <identification>
            <name>CSEImproveCA</name>
            <description>Represents the Service Assembly of CSEImproveCA</description>
        </identification>
        <connections>
            <connection>
                <consumer endpoint-name="FileImport" service-name="ns1:casaService1"/>
                <provider endpoint-name="LSFSynchronizationMessagePortTypeRole_myRole" service-name="ns2:LSFPLink"/>
            </connection>
            <connection>
                <consumer endpoint-name="LSFSynchronizationMessagePort" service-name="ns3:LSFSynchronizationMessageService"/>
                <provider endpoint-name="LSFSynchronizationMessagePortTypeRole_myRole" service-name="ns2:LSFPLink"/>
            </connection>
            <connection>
                <consumer endpoint-name="CoursePortTypeBindingPort" service-name="ns4:CourseService"/>
                <provider endpoint-name="CoursePortTypeRole_myRole" service-name="ns5:CoursePLink"/>
            </connection>
            <connection>
                <consumer endpoint-name="SynchronizationMessagePort" service-name="ns6:SynchronizationMessageService"/>
                <provider endpoint-name="SynchronizationMessagePortTypeRole_myRole" service-name="ns7:SyncMsgPLink"/>
            </connection>
            <connection>
                <consumer endpoint-name="SynchronizationMessagePortTypeRole_partnerRole" service-name="ns2:SyncMsgPLink"/>
                <provider endpoint-name="SynchronizationMessagePortTypeRole_myRole" service-name="ns7:SyncMsgPLink"/>
            </connection>
            <connection>
                <consumer endpoint-name="CoursePortTypeRole_partnerRole" service-name="ns7:CoursePLink"/>
                <provider endpoint-name="CoursePortTypeRole_myRole" service-name="ns5:CoursePLink"/>
            </connection>
        </connections>
        <connections xmlns="http://www.sun.com/jbi/qos">
            <connection>
                <consumer endpoint-name="FileImport" service-name="ns1:casaService1"/>
                <provider endpoint-name="LSFSynchronizationMessagePortTypeRole_myRole" service-name="ns2:LSFPLink"/>
                <redelivery xmlns="http://www.sun.com/jbi/qos/redelivery" maxAttempts="-1" waitTime="1000">
                <on-failure>
                    <delete/>
                </on-failure>
            </redelivery>
                <throttling xmlns="http://www.sun.com/jbi/qos/throttling" maximumConcurrencyLimit="1"/>
            </connection>
        </connections>
    </service-assembly>
</jbi>
