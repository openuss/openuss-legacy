<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN" "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>
	
	
	<managed-bean>
		<managed-bean-name>ACL</managed-bean-name>
		<managed-bean-class>org.openuss.security.acl.LectureAclEntry</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>newsBean</managed-bean-name>
		<managed-bean-class>org.openuss.web.lecture.NewsBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	
	<!-- DataTableModels -->

 	<managed-bean>
 		<managed-bean-name>facultySecurity</managed-bean-name>
 		<managed-bean-class>org.openuss.lecture.FacultySecurity</managed-bean-class>
 		<managed-bean-scope>request</managed-bean-scope>
 	</managed-bean>

 	<managed-bean>
 		<managed-bean-name>accessType</managed-bean-name>
 		<managed-bean-class>org.openuss.lecture.AccessType</managed-bean-class>
 		<managed-bean-scope>application</managed-bean-scope>
 	</managed-bean>
 	
 	<managed-bean>
 		<managed-bean-name>facultyMember</managed-bean-name>
 		<managed-bean-class>org.openuss.lecture.FacultyMember</managed-bean-class>
 		<managed-bean-scope>request</managed-bean-scope>
 	</managed-bean>
 	
 	<managed-bean>
 		<managed-bean-name>facultyGroup</managed-bean-name>
 		<managed-bean-class>org.openuss.lecture.FacultyGroup</managed-bean-class>
 		<managed-bean-scope>request</managed-bean-scope>
 	</managed-bean>
 	
 	<converter>
 		<converter-id>facultyConverter</converter-id>
 		<converter-class>org.openuss.web.lecture.converter.FacultyConverter</converter-class>
 	</converter>
 	
 	<!-- Managed Beans -->
 	<managed-bean>
 		<managed-bean-name>faculty</managed-bean-name>
 		<managed-bean-class>org.openuss.lecture.FacultyImpl</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope>
 	</managed-bean>

 	<managed-bean>
 		<managed-bean-name>enrollment</managed-bean-name>
 		<managed-bean-class>org.openuss.lecture.EnrollmentImpl</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope>
 	</managed-bean>
 	
 	<managed-bean>
 		<managed-bean-name>enrollmentInfo</managed-bean-name>
 		<managed-bean-class>org.openuss.lecture.EnrollmentInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope>
 	</managed-bean>

 	<managed-bean>
 		<managed-bean-name>documents_current_folder</managed-bean-name>
 		<managed-bean-class>org.openuss.documents.FolderInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope>
 	</managed-bean>

 	<managed-bean>
 		<managed-bean-name>documents_selected_folder</managed-bean-name>
 		<managed-bean-class>org.openuss.documents.FolderInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope>
 	</managed-bean>

 	<managed-bean>
 		<managed-bean-name>documents_selected_file</managed-bean-name>
 		<managed-bean-class>org.openuss.documents.FileInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope>
 	</managed-bean>

 	<managed-bean>
 		<managed-bean-name>documents_entry_selection</managed-bean-name>
 		<managed-bean-class>org.openuss.web.documents.FolderEntrySelection</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope>
 	</managed-bean>

 	<managed-bean>
 		<managed-bean-name>discussion_discussionentry</managed-bean-name>
 		<managed-bean-class>org.openuss.discussion.PostInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope>
 	</managed-bean>
 	
 	<managed-bean>
 		<managed-bean-name>discussion_topic</managed-bean-name>
 		<managed-bean-class>org.openuss.discussion.TopicInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope>
 	</managed-bean>
 	
 	<managed-bean>
 		<managed-bean-name>braincontest_answer</managed-bean-name>
 		<managed-bean-class>org.openuss.web.braincontest.AnswerWebInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope> 	
 	</managed-bean>

 	<managed-bean>
 		<managed-bean-name>braincontest_contest</managed-bean-name>
 		<managed-bean-class>org.openuss.braincontest.BrainContestInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope> 	
 	</managed-bean>

 	<managed-bean>
 		<managed-bean-name>news_selected_newsitem</managed-bean-name>
 		<managed-bean-class>org.openuss.news.NewsItemInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope> 	
 	</managed-bean>
 	
</faces-config>