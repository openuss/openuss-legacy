<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN" "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>

  <!-- - - - - - - - - - - - - - - - - - - - - - - - - -  
    	G l o b a l - R u l e s
    - - - - - - - - - - - - - - - - - - - - - - - - - -->	

	<navigation-rule>
		<description>Global navigation rules</description>
		<from-view-id>/*</from-view-id>

		<navigation-case>
			<from-outcome>desktop</from-outcome>
			<to-view-id>/views/secured/desktop/desktop.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/views/welcome.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/views/public/login/login.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/views/public/login/logout.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>messagebox</from-outcome>
			<to-view-id>/views/public/messagebox.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>forgot_password</from-outcome>
			<to-view-id>/views/public/login/forgotPassword.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>register_user</from-outcome>
			<to-view-id>/views/public/user/registration/startProcess.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>search_faculties</from-outcome>
			<to-view-id>/views/public/faculty/faculties.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>faculty</from-outcome>
			<to-view-id>/views/secured/lecture/faculty.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>user_profile</from-outcome>
			<to-view-id>/views/secured/user/profile.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>user_logindata</from-outcome>
			<to-view-id>/views/secured/user/logindata.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>user_contactdata</from-outcome>
			<to-view-id>/views/secured/user/contactdata.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>user_preferences</from-outcome>
			<to-view-id>/views/secured/user/preferences.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>user_profile_view</from-outcome>
			<to-view-id>/views/public/user/userprofile.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>closePopupFrame</from-outcome>
			<to-view-id>/views/public/popup/closePopupFrame.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>administration</from-outcome>
			<to-view-id>/views/secured/system/properties.xhtml</to-view-id>
		</navigation-case>
		
	
		<navigation-case>
			<from-outcome>secure_password_change</from-outcome>
			<to-view-id>/views/secured/user/newPassword.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>enrollment_main</from-outcome>
			<to-view-id>/views/secured/enrollment/main.xhtml?enrollmentId=#{enrollment.id}</to-view-id>
		</navigation-case>
		
	</navigation-rule>
	
  <!-- - - - - - - - - - - - - - - - - - - - - - - - - -  
    	L o g i n / L o g o u t  - R u l e s
    - - - - - - - - - - - - - - - - - - - - - - - - - -->	

	<navigation-rule>
		<from-view-id>/views/public/login/forgotPassword.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/views/public/messagebox.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/views/welcome.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
  <!-- - - - - - - - - - - - - - - - - - - - - - - - - -  
       R e g i s t r a t i o n  - P r o c e s s 
    - - - - - - - - - - - - - - - - - - - - - - - - - -->	
    
    <navigation-rule>
    	<from-view-id>/views/public/user/registration/*</from-view-id>
	   	<navigation-case>
    		<from-outcome>start</from-outcome>
    		<to-view-id>/views/public/user/registration/startProcess.xhtml</to-view-id>
    	</navigation-case>
	   	<navigation-case>
    		<from-outcome>step1</from-outcome>
    		<to-view-id>/views/public/user/registration/step1UserAgreement.xhtml</to-view-id>
    	</navigation-case>
	   	<navigation-case>
    		<from-outcome>step2</from-outcome>
    		<to-view-id>/views/public/user/registration/step2LoginData.xhtml</to-view-id>
    	</navigation-case>
	   	<navigation-case>
    		<from-outcome>step3</from-outcome>
    		<to-view-id>/views/public/user/registration/step3ContactData.xhtml</to-view-id>
    	</navigation-case>
	   	<navigation-case>
    		<from-outcome>step4</from-outcome>
    		<to-view-id>/views/public/user/registration/step4AssistantRole.xhtml</to-view-id>
    	</navigation-case>
	   	<navigation-case>
    		<from-outcome>step5</from-outcome>
    		<to-view-id>/views/public/user/registration/step5Preferences.xhtml</to-view-id>
    	</navigation-case>
	   	<navigation-case>
    		<from-outcome>step6</from-outcome>
    		<to-view-id>/views/public/user/registration/step6Confirmation.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>

    
  <!-- - - - - - - - - - - - - - - - - - - - - - - - - -  
       A c t i o n s
    - - - - - - - - - - - - - - - - - - - - - - - - - -->	
    <navigation-rule>
    	<from-view-id>/actions/public/user/activate.xhtml</from-view-id>
    	<navigation-case>
    		<from-action>userActivate.activate</from-action>
    		<from-outcome>success</from-outcome>
    		<to-view-id>/views/secured/desktop/desktop.xhtml</to-view-id>
    	</navigation-case>
    	<navigation-case>
    		<from-action>userActivate.activate</from-action>
    		<from-outcome>failure</from-outcome>
    		<to-view-id>/views/public/user/activate/request.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
    
    <navigation-rule>
    	<from-view-id>/actions/public/user/password/change.xhtml</from-view-id>
    	<navigation-case>
    		<from-action>passwordChangeAction.loginUserByCode</from-action>
    		<from-outcome>success</from-outcome>
    		<to-view-id>/views/secured/user/newPassword.xhtml</to-view-id>
    	</navigation-case>
    	<navigation-case>
    		<from-action>passwordChangeAction.loginUserByCode</from-action>
    		<from-outcome>failure</from-outcome>
    		<to-view-id>/views/public/login/forgotPassword.xhtml</to-view-id>
    	</navigation-case>
    
    </navigation-rule>

  <!-- - - - - - - - - - - - - - - - - - - - - - - - - -  
       D e s k t o p  - P a g es
    - - - - - - - - - - - - - - - - - - - - - - - - - -->	
	<navigation-rule>
		<from-view-id>/views/secured/*</from-view-id>
		<navigation-case>
			<from-outcome>user_bookmarks</from-outcome>
			<to-view-id>/views/sercured/user/profile.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>faculty_create</from-outcome>
			<to-view-id>/views/secured/lecture/registration/start.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/views/secured/user/newPassword.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/views/secured/desktop/desktop</to-view-id>
		</navigation-case>
	</navigation-rule>
	
    <navigation-rule>
    	<from-view-id>/views/secured/desktop/*</from-view-id>
    	<navigation-case>
    		<from-outcome>subject_enrollment_selection</from-outcome>
    		<to-view-id>/views/secured/desktop/subjectenrollmenselection.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
  <!-- - - - - - - - - - - - - - - - - - - - - - - - - -  
       F a c u l t y  - R e g i s t r a t i o n 
    - - - - - - - - - - - - - - - - - - - - - - - - - -->	
	<navigation-rule>
		<from-view-id>/views/secured/lecture/registration/*</from-view-id>

		<navigation-case>
			<from-outcome>faculty_registration_start</from-outcome>
			<to-view-id>/views/secured/lecture/registration/start.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>faculty_registration_step1</from-outcome>		
			<to-view-id>/views/secured/lecture/registration/step1.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>faculty_registration_step2</from-outcome>		
			<to-view-id>/views/secured/lecture/registration/step2.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

  <!-- - - - - - - - - - - - - - - - - - - - - - - - - -  
       F a c u l t y  - P a g e
    - - - - - - - - - - - - - - - - - - - - - - - - - -->	

	<navigation-rule>
		<from-view-id>/views/secured/lecture/*</from-view-id>

		<navigation-case>
			<from-outcome>faculty</from-outcome>
			<to-view-id>/views/secured/lecture/faculty.xtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>faculty_data</from-outcome>
			<to-view-id>/views/secured/lecture/facultyoptions.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>faculty_members</from-outcome>
			<to-view-id>/views/secured/lecture/auth/members.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>faculty_aspirants</from-outcome>
			<to-view-id>/views/secured/lecture/auth/aspirants.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>faculty_news</from-outcome>
			<to-view-id>/views/secured/lecture/news.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>faculty_news_edit</from-outcome>
			<to-view-id>/views/secured/lecture/newsedit.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>faculty_subjects</from-outcome>
			<to-view-id>/views/secured/lecture/subjects.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>faculty_subject_remove</from-outcome>
			<to-view-id>/views/secured/lecture/subjectremove.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>faculty_periods</from-outcome>
			<to-view-id>/views/secured/lecture/periods.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>faculty_period</from-outcome>
			<to-view-id>/views/secured/lecture/periodedit.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>faculty_period_remove</from-outcome>
			<to-view-id>/views/secured/lecture/periodremove.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>faculty_enrollment_remove</from-outcome>
			<to-view-id>/views/secured/lecture/enrollmentremove.xhtml</to-view-id>
		</navigation-case>
		
		
	</navigation-rule>
	
  <!-- - - - - - - - - - - - - - - - - - - - - - - - - -  
       E n r o l l m e n t - P a g e
    - - - - - - - - - - - - - - - - - - - - - - - - - -->		
		<navigation-rule>
			<from-view-id>/views/secured/*</from-view-id>
		
			<!-- TODO move main to public or insert second public enrollment main -->
			<navigation-case>
				<from-outcome>enrollment</from-outcome>
				<to-view-id>/views/secured/enrollment/main.xhtml</to-view-id>
			</navigation-case>

			<navigation-case>
				<from-outcome>enrollment_documents</from-outcome>
				<to-view-id>/views/secured/enrollment/documents.xhtml</to-view-id>
			</navigation-case>
	
			<navigation-case>
				<from-outcome>enrollment_discussion</from-outcome>
				<to-view-id>/views/secured/enrollment/discussion.xhtml</to-view-id>
			</navigation-case>
	
			<navigation-case>
				<from-outcome>enrollment_mailinglist</from-outcome>
				<to-view-id>/views/secured/enrollment/mailinglist.xhtml</to-view-id>
			</navigation-case>
	
			<navigation-case>
				<from-outcome>enrollment_quiz</from-outcome>
				<to-view-id>/views/secured/enrollment/quiz.xhtml</to-view-id>
			</navigation-case>
	
			<navigation-case>
				<from-outcome>enrollment_chat</from-outcome>
				<to-view-id>/views/secured/enrollment/chat.xhtml</to-view-id>
			</navigation-case>
	
			<navigation-case>
				<from-outcome>enrollment_wiki</from-outcome>
				<to-view-id>/views/secured/enrollment/wiki.xhtml</to-view-id>
			</navigation-case>
	
			<navigation-case>
				<from-outcome>enrollment_options</from-outcome>
				<to-view-id>/views/secured/enrollment/options.xhtml</to-view-id>
			</navigation-case>
			
			<navigation-case>
				<from-outcome>enrollment_options_config</from-outcome>
				<to-view-id>/views/secured/enrollment/enrollmentoptions.xhtml</to-view-id>				
			</navigation-case>

			<navigation-case>
				<from-outcome>enrollment_options_aspirants</from-outcome>
				<to-view-id>/views/secured/enrollment/enrollmentaspirants.xhtml</to-view-id>				
			</navigation-case>
			
			<navigation-case>
				<from-outcome>enrollment_options_members</from-outcome>
				<to-view-id>/views/secured/enrollment/enrollmentparticipants.xhtml</to-view-id>				
			</navigation-case>

			<navigation-case>
				<from-outcome>enrollment_options_assistants</from-outcome>
				<to-view-id>/views/secured/enrollment/enrollmentassistants.xhtml</to-view-id>				
			</navigation-case>

			<navigation-case>
				<from-outcome>enrollment_options_news</from-outcome>
				<to-view-id>/views/secured/lecture/news.xhtml</to-view-id>				
			</navigation-case>

			<navigation-case>
				<from-outcome>mailinglist_newmail</from-outcome>
				<to-view-id>/views/secured/enrollment/newmail.xhtml</to-view-id>				
			</navigation-case>			

			<navigation-case>
				<from-outcome>mailinglist_subscribers</from-outcome>
				<to-view-id>/views/secured/enrollment/mailinglistsubscribers.xhtml</to-view-id>				
			</navigation-case>			

			<navigation-case>
				<from-outcome>braincontest_main</from-outcome>
				<to-view-id>/views/secured/enrollment/braincontestmain.xhtml</to-view-id>				
			</navigation-case>			

			<navigation-case>
				<from-outcome>braincontest_new</from-outcome>
				<to-view-id>/views/secured/enrollment/braincontestnew.xhtml</to-view-id>				
			</navigation-case>			

			<navigation-case>
				<from-outcome>braincontest_solve</from-outcome>
				<to-view-id>/views/secured/enrollment/braincontestsolve.xhtml</to-view-id>				
			</navigation-case>			

			<navigation-case>
				<from-outcome>braincontest_top</from-outcome>
				<to-view-id>/views/secured/enrollment/braincontesttop.xhtml</to-view-id>				
			</navigation-case>			
			
		</navigation-rule>
	
  <!-- - - - - - - - - - - - - - - - - - - - - - - - - -  
       News  - P a g e
    - - - - - - - - - - - - - - - - - - - - - - - - - -->		
	<navigation-rule>
		<from-view-id>*</from-view-id>
		
		<navigation-case>
			<from-outcome>detailsNewsItem</from-outcome>
			<to-view-id>/views/public/news/newsDetail.xhtml</to-view-id>
		</navigation-case>

	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/views/public/news/newsDetail.xhtml</from-view-id>
		
		<navigation-case>
			<from-outcome>newsItem_edit</from-outcome>
			<to-view-id>/views/public/news/newsEdit.xhtml</to-view-id>
		</navigation-case>

	</navigation-rule>
	
  <!-- - - - - - - - - - - - - - - - - - - - - - - - - -  
       Administration - P a g e
    - - - - - - - - - - - - - - - - - - - - - - - - - -->		
	<navigation-rule>
		<from-view-id>/views/secured/system/*</from-view-id>
		
		<navigation-case>
			<from-outcome>admin_properties</from-outcome>
			<to-view-id>/views/secured/system/properties.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>admin_users</from-outcome>
			<to-view-id>/views/secured/system/userbrowser.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>admin_groups</from-outcome>
			<to-view-id>/views/secured/system/groups.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>admin_faculties</from-outcome>
			<to-view-id>/views/secured/system/faculties.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>user_profile_view</from-outcome>
			<to-view-id>/views/public/user/userprofile.xhtml</to-view-id>
		</navigation-case>

	</navigation-rule>
	
</faces-config>

