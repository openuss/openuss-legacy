<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:jaxws="http://cxf.apache.org/jaxws"
	xmlns:cxf="http://cxf.apache.org/core"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd
http://cxf.apache.org/core http://cxf.apache.org/schemas/core.xsd">

	<import resource="classpath:META-INF/cxf/cxf.xml" />
	<import resource="classpath:META-INF/cxf/cxf-extension-soap.xml" />
	<import resource="classpath:META-INF/cxf/cxf-extension-http.xml" />
	<import	resource="classpath:META-INF/cxf/cxf-extension-http-jetty.xml" />

	<jaxws:endpoint id="lectureService"
		implementor="org.openuss.services.LectureWebServiceMock"
		address="http://localhost:9000/plexus-web/lectureService">
		<jaxws:features>
			<cxf:logging/>
		</jaxws:features>
	</jaxws:endpoint>

	  
	<jaxws:client id="lectureClient"
		serviceClass="org.openuss.services.LectureWebService"
		address="http://localhost:9000/plexus-web/lectureService">
		<jaxws:features>
			<cxf:logging/>
		</jaxws:features>
	</jaxws:client>  
	  
</beans>