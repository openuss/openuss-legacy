<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
    <class name="org.openuss.newsletter.MailImpl" table="NEWSLETTER_MAIL" dynamic-insert="false" dynamic-update="false">
        <cache usage="read-write" />
        <id name="id" type="java.lang.Long" unsaved-value="null">
            <column name="ID" sql-type="BIGINT"/>
            <generator class="sequence">
                <param name="sequence">GLOBAL_SEQUENCE</param>
            </generator>
        </id>
        <property name="status" type="org.openuss.newsletter.MailingStatusEnum">
            <column name="STATUS" not-null="true" unique="false" sql-type="INTEGER"/>
        </property>
        <property name="subject" type="java.lang.String">
            <column name="SUBJECT" not-null="true" unique="false" sql-type="VARCHAR(250)"/>
        </property>
        <property name="text" type="org.andromda.persistence.hibernate.usertypes.HibernateStringClobType">
            <column name="TEXT" not-null="true" unique="false" sql-type="BLOB"/>
        </property>
        <property name="sms" type="boolean">
            <column name="SMS" not-null="true" unique="false" sql-type="SMALLINT"/>
        </property>
        <property name="sendDate" type="java.util.Date">
            <column name="SEND_DATE" not-null="true" unique="false" sql-type="TIMESTAMP"/>
        </property>
        <property name="commandId" type="java.lang.Long">
            <column name="COMMAND_ID" not-null="false" unique="false" sql-type="BIGINT"/>
        </property>
        <property name="messageId" type="java.lang.Long">
            <column name="MESSAGE_ID" not-null="false" unique="false" sql-type="BIGINT"/>
        </property>
        <many-to-one name="newsletter" class="org.openuss.newsletter.NewsletterImpl" foreign-key="NEWSLETTER_MAIL_NEWSLETTER_FKC" lazy="proxy" fetch="select">
            <column name="NEWSLETTER_FK" not-null="true" sql-type="BIGINT"/>
        </many-to-one>
    </class>
</hibernate-mapping>