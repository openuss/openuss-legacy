<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
    <class name="org.openuss.commands.CommandImpl" table="COMMAND_COMMAND" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="java.lang.Long" unsaved-value="null">
            <column name="ID" sql-type="BIGINT"/>
            <generator class="sequence">
                <param name="sequence">GLOBAL_SEQUENCE</param>
            </generator>
        </id>
        <property name="domainIdentifier" type="java.lang.Long">
            <column name="DOMAIN_IDENTIFIER" not-null="true" unique="false" sql-type="BIGINT"/>
        </property>
        <property name="command" type="java.lang.String">
            <column name="COMMAND" not-null="true" unique="false" sql-type="VARCHAR(64)"/>
        </property>
        <property name="state" type="org.openuss.commands.CommandStateEnum">
            <column name="STATE" not-null="true" unique="false" sql-type="INTEGER"/>
        </property>
        <property name="commandType" type="java.lang.String">
            <column name="COMMAND_TYPE" not-null="false" unique="false" sql-type="VARCHAR(64)"/>
        </property>
        <property name="startTime" type="java.util.Date">
            <column name="START_TIME" not-null="false" unique="false" sql-type="TIMESTAMP"/>
        </property>
        <property name="executionTime" type="java.util.Date">
            <column name="EXECUTION_TIME" not-null="false" unique="false" sql-type="TIMESTAMP"/>
        </property>
    </class>
</hibernate-mapping>