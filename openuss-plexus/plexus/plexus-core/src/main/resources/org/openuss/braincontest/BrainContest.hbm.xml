<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
    <class name="org.openuss.braincontest.BrainContestImpl" table="BRAINCONTEST" dynamic-insert="false" dynamic-update="false">
        <cache usage="read-write" />
        <id name="id" type="java.lang.Long" unsaved-value="null">
            <column name="ID" sql-type="BIGINT"/>
            <generator class="sequence">
                <param name="sequence">GLOBAL_SEQUENCE</param>
            </generator>
        </id>
        <property name="domainIdentifier" type="java.lang.Long">
            <column name="DOMAIN_IDENTIFIER" not-null="true" unique="false" sql-type="BIGINT"/>
        </property>
        <property name="releaseDate" type="java.util.Date">
            <column name="RELEASE_DATE" not-null="true" unique="false" sql-type="TIMESTAMP"/>
        </property>
        <property name="description" type="org.andromda.persistence.hibernate.usertypes.HibernateStringClobType">
            <column name="DESCRIPTION" not-null="true" unique="false" sql-type="BLOB"/>
        </property>
        <property name="title" type="java.lang.String">
            <column name="TITLE" not-null="true" unique="false" sql-type="VARCHAR(500)"/>
        </property>
        <property name="tries" type="java.lang.Integer">
            <column name="TRIES" not-null="true" unique="false" sql-type="INTEGER"/>
        </property>
        <property name="solution" type="java.lang.String">
            <column name="SOLUTION" not-null="true" unique="false" sql-type="VARCHAR(500)"/>
        </property>
        <bag name="answers" order-by="ANSWERED_AT" lazy="false" fetch="select" inverse="false">
            <cache usage="read-write" />
            <key foreign-key="BRAINCONTEST_ANSWER_CONTEST_IC">
                <column name="CONTEST_ID" sql-type="BIGINT"/>
            </key>
            <one-to-many class="org.openuss.braincontest.AnswerImpl"/>
        </bag>
    </class>
</hibernate-mapping>