<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

    <!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - 
    		Cluster Command Processor Test Configuration
     - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
     
     <bean id="commandProcessor" class="org.openuss.commands.ClusterCommandProcessor">
     	<property name="commandDao"><ref bean="mockCommandDao"/></property>
     	<property name="lastProcessedCommandDao"><ref bean="mockLastProcessedCommandDao"/></property>
     	<property name="systemService"><ref bean="mockSystemService120"/></property>
     </bean>
     
     
     <bean id="mockCommand" class="org.openuss.commands.MockCommand" singleton="true"/>
     
     <bean id="mockCommandDao" class="org.openuss.commands.MockCommandDao"/>
     
     <bean id="mockLastProcessedCommandDao" class="org.openuss.commands.MockLastProcessedCommandDao"/>
     
     <bean id="mockSystemService120" class="org.openuss.commands.MockServiceSystem">
     	<property name="instanceIdentity" value="120"/>
     </bean>
     
</beans>
