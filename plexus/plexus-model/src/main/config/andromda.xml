        		<property name="forms">${web.generated.dir}</property>					
        		<property name="enableFaceletsDevelopmentSupport">true</property>		
        		<property name="viewType">facelets</property>
        	</properties>																
        </namespace>																	
 -->        
 
    </namespaces>
</andromda>
