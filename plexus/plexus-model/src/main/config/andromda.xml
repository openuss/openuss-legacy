<andromda>
    <properties>
        <property name="modelValidation">true</property>
        <property name="cartridgeFilter">${filter}</property>
    </properties>

    <server>
        <host>localhost</host>
        <port>4446</port>
    </server>
    
    <repositories>
        <repository name="netBeansMDR">
            <models>
                <model>
                    <uri>${model.uri}</uri>
                    <moduleSearchLocations>
                        <location patterns="**/*.xml.zip, **/*.uml2">${settings.localRepository}/org/andromda/profiles</location>
                        <location patterns="**/*.xml.zip, **/*.uml2">${project.build.sourceDirectory}</location>
                    </moduleSearchLocations>
                    <modelPackages processAll="true">
                    	<modelPackage process="false">org::acegisecurity::acl::basic::cache</modelPackage>
                    	<modelPackage process="false">org::acegisecurity::providers::dao</modelPackage>
                    	<modelPackage process="false">org::openuss::events</modelPackage>
                    	<modelPackage process="false">org::openuss::framework::*</modelPackage>
                    	<modelPackage process="false">org::openuss::migration::legacy::dao</modelPackage>
                    	<modelPackage process="false">org::quartz</modelPackage>
                    	<modelPackage process="false">architecture::*</modelPackage>
                    	<modelPackage process="false">java::*</modelPackage>
                    </modelPackages>
                </model>
            </models>
        </repository>
    </repositories>
    
    
    <namespaces>
        <namespace name="default">
            <properties>
				<property name="mergeLocation">${tools.andromda.dir}</property>            
                <property name="mergeMappingsUri">file:${tools.andromda.dir}/mappings/FoundationMergeMappings.xml</property>
                
                <property name="languageMappingsUri">file:${tools.andromda.dir}/mappings/JavaMappings.xml</property>
                <property name="wrapperMappingsUri">JavaWrapper</property>
                <property name="sqlMappingsUri">file:${tools.andromda.dir}/mappings/FirebirdMappings.xml</property>
                <property name="jdbcMappingsUri">JDBC</property>
                <property name="maxSqlNameLength">30</property>
                <property name="foreignKeySuffix">_FK</property>

                
                <property name="enableTemplating">true</property>
                <property name="defaultCollectionInterfaces">java.util.List</property>
                <property name="specificCollectionInterfaces">true</property>
                
                <!-- Hibernate cache (default) configuration -->
                <property name="hibernateVersion">3</property>
                <property name="cache">${core.generated.dir}</property>
                <property name="hibernateEnableCache">true</property>
                <property name="hibernateEhCacheDiskStore">java.io.tmpdir/hibernate</property>
                <property name="hibernateCacheProvider">org.hibernate.cache.EhCacheProvider</property>                
                <property name="hibernateEntityCache">none</property>
                <property name="hibernateAssociationCache">read-write</property>
                <property name="hibernateQueryCacheFactory">org.hibernate.cache.StandardQueryCacheFactory</property>
                <property name="hibernateUseQueryCache">true</property>
                <property name="ehCacheMaxElementsInMemory">10000</property>
                <property name="ehCacheEternal">false</property>
                <property name="ehCacheTimeToIdleSeconds">240</property>
                <property name="ehCacheTimeToLiveSeconds">240</property>
                <property name="ehCacheOverflowToDisk">false</property>

            </properties>
        </namespace>
        
        <namespace name="spring">
            <properties>
                <property name="mergeMappingsUri">file:${tools.andromda.dir}/mappings/SpringMergeMappings.xml</property>
                
                <property name="tests">${core.tests.generated.dir}</property>
                <property name="test-impls">${core.tests.manual.dir}</property>

            	<!-- Spring Datasource Configuration -->

            	<property name="dataSource">java:comp/env/jdbc/openuss30</property>
	          	<property name="username">sysdba</property> 
                <property name="password">masterkey</property>
                <property name="driver">org.firebirdsql.jdbc.FBDriver</property>
                <property name="connectionUrl">jdbc:firebirdsql:localhost:openuss30</property>

                <!-- Spring Outlets Configuration -->

                <property name="daos">${core.generated.dir}</property>
                <property name="dao-impls">${core.manual.dir}</property>
                <property name="advice-impls">${core.manual.dir}</property>
                <property name="spring-configuration">${core.generated.dir}</property>
                <property name="services">${core.generated.dir}</property>
                <property name="service-impls">${core.manual.dir}</property>
                <property name="service-interfaces">${api.generated.dir}</property>
                <property name="value-objects">${api.generated.dir}</property>
                <property name="criteria">${api.generated.dir}</property>

                <!-- Spring Hibernate Configuraiton  -->

				<property name="jdbcDataSourceClass">org.apache.commons.dbcp.BasicDataSource</property>
				<property name="jdbcDataSourceDestroyMethod">close</property>
                <property name="hibernateDialect">org.hibernate.dialect.FirebirdDialect</property>
                <property name="hibernateHbm2DDLAuto">auto</property>
                <property name="hibernateShowSql">false</property>
                <property name="parameterRequiredCheck">false</property>
                <property name="daoInheritanceEnabled">false</property>

                <!-- Spring Configuration for proper support of hibernate 3.x -->

                <property name="sessionFactory">org.springframework.orm.hibernate3.LocalSessionFactoryBean</property>
				
                <property name="localTransactionManager">org.springframework.orm.hibernate3.HibernateTransactionManager</property>
                <property name="transactionManager">org.springframework.orm.hibernate3.HibernateTransactionManager</property>

                <!-- Spring Security Configuration -->
                <property name="acegiServiceSecurity">true</property>
                <property name="acegiAuthenticationManager">authenticationManager</property>

            </properties>
        </namespace>
        
        <namespace name="hibernate">
            <properties>
                <property name="mergeMappingsUri">file:${tools.andromda.dir}/mappings/HibernateMergeMappings.xml</property>

                <property name="customTypesPackage">org.openuss.framework.persistence.hibernate</property>
                <property name="defaultHibernateGeneratorClass">sequence</property>
                <property name="hibernateTypeMappingsUri">file:${tools.andromda.dir}/mappings/HibernateTypeMappings.xml</property>
                <property name="entities">${core.generated.dir}</property>
                <property name="entity-impls">${core.manual.dir}</property>   
                <property name="entity-interfaces">${api.generated.dir}</property>
                <property name="entity-mappings">${core.generated.dir}</property>
                <property name="user-types">${core.generated.dir}</property>
                
            </properties>
        </namespace>

        <namespace name="java">
            <properties>
                <property name="mergeMappingsUri">file:${tools.andromda.dir}/mappings/JavaMergeMappings.xml</property>
                <property name="exceptions">${api.generated.dir}</property>
                <property name="value-objects">${api.generated.dir}</property>
                <property name="enumerations">${api.generated.dir}</property>
            </properties>
        </namespace>
