<project name="openuss :: plexus :: model :: build" default="generate-sources" basedir=".">
	<target name="generate-sources">
		<exec executable="mvn.bat">
			<arg line="-e -o generate-sources"/>
		</exec>
	</target>
	<target name="generate-sources ORG::OPENUSS::SYSTEM">
		<exec executable="mvn.bat">
			<arg line="-e -o generate-sources -Dandromda.config=andromda-system.xml "/>
		</exec>
	</target>
	<target name="generate-sources ORG::OPENUSS::MAILINGLIST">
		<exec executable="mvn.bat">
			<arg line="-e -o generate-sources -Dandromda.config=andromda-mailinglist.xml "/>
		</exec>
	</target>
	<target name="compile">
		<exec executable="mvn.bat">
			<arg line="compile"/>
		</exec>
	</target>
	<target name="andromda :: start-server">
		<exec executable="mvn.bat">
			<arg line="andromda:start-server"/>
		</exec>
	</target>
	<target name="andromda :: stop-server">
		<exec executable="mvn.bat">
			<arg line="andromda:stop-server"/>
		</exec>
	</target>
	
</project>