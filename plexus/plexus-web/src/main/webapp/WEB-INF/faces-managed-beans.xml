<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN" "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>
	
	<managed-bean>
		<managed-bean-name>ACL</managed-bean-name>
		<managed-bean-class>org.openuss.security.acl.LectureAclEntry</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>newsBean</managed-bean-name>
		<managed-bean-class>org.openuss.web.lecture.NewsBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>showuser</managed-bean-name>
		<managed-bean-class>org.openuss.security.UserImpl</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<!-- DataTableModels -->
 	<managed-bean>
 		<managed-bean-name>applicationInfo</managed-bean-name>
 		<managed-bean-class>org.openuss.lecture.ApplicationInfo</managed-bean-class>
 		<managed-bean-scope>request</managed-bean-scope>
 		<managed-property>
 			<property-name>departmentInfo</property-name>
 			<value>#{applicationDeparment}</value>
 		</managed-property>
 	</managed-bean>

 	<managed-bean>
 		<managed-bean-name>applicationDeparment</managed-bean-name>
 		<managed-bean-class>org.openuss.lecture.DepartmentInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope>
 	</managed-bean>
 	
 	<managed-bean>
 		<managed-bean-name>instituteSecurity</managed-bean-name>
 		<managed-bean-class>org.openuss.lecture.InstituteSecurity</managed-bean-class>
 		<managed-bean-scope>request</managed-bean-scope>
 	</managed-bean>

 	<managed-bean>
 		<managed-bean-name>accessType</managed-bean-name>
 		<managed-bean-class>org.openuss.lecture.AccessType</managed-bean-class>
 		<managed-bean-scope>application</managed-bean-scope>
 	</managed-bean>
 	
 	<managed-bean>
 		<managed-bean-name>instituteMember</managed-bean-name>
 		<managed-bean-class>org.openuss.lecture.InstituteMember</managed-bean-class>
 		<managed-bean-scope>request</managed-bean-scope>
 	</managed-bean>
 	
 	<managed-bean>
 		<managed-bean-name>instituteGroup</managed-bean-name>
 		<managed-bean-class>org.openuss.lecture.InstituteGroup</managed-bean-class>
 		<managed-bean-scope>request</managed-bean-scope>
 	</managed-bean>
 	
 	<converter>
 		<converter-id>instituteConverter</converter-id>
 		<converter-class>org.openuss.web.lecture.converter.InstituteConverter</converter-class>
 	</converter>
 	
 	<!-- Managed Beans -->
 	<managed-bean>
 		<managed-bean-name>institute</managed-bean-name>
 		<managed-bean-class>org.openuss.lecture.InstituteImpl</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope>
 	</managed-bean>
 	
 	<managed-bean>
 		<managed-bean-name>periodInfo</managed-bean-name>
 		<managed-bean-class>org.openuss.lecture.PeriodInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope>
 	</managed-bean>
 	
 	<managed-bean>
 		<managed-bean-name>departmentInfo</managed-bean-name>
 		<managed-bean-class>org.openuss.lecture.DepartmentInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope>
 	</managed-bean>
 	
 	<managed-bean>
 		<managed-bean-name>universityInfo</managed-bean-name>
 		<managed-bean-class>org.openuss.lecture.UniversityInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope>
 	</managed-bean>
 	
 	<managed-bean>
 		<managed-bean-name>instituteInfo</managed-bean-name>
 		<managed-bean-class>org.openuss.lecture.InstituteInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope>
 	</managed-bean>

 	<managed-bean>
 		<managed-bean-name>course</managed-bean-name>
 		<managed-bean-class>org.openuss.lecture.CourseInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope>
 	</managed-bean>
 	
 	<managed-bean>
 		<managed-bean-name>courseInfo</managed-bean-name>
 		<managed-bean-class>org.openuss.lecture.CourseInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope>
 	</managed-bean>
 	
 	<managed-bean>
 		<managed-bean-name>courseTypeInfo</managed-bean-name>
 		<managed-bean-class>org.openuss.lecture.CourseTypeInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope>
 	</managed-bean>
 	


 	<managed-bean>
 		<managed-bean-name>documents_current_folder</managed-bean-name>
 		<managed-bean-class>org.openuss.documents.FolderInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope>
 	</managed-bean>

 	<managed-bean>
 		<managed-bean-name>documents_selected_folder</managed-bean-name>
 		<managed-bean-class>org.openuss.documents.FolderInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope>
 	</managed-bean>

 	<managed-bean>
 		<managed-bean-name>documents_selected_file</managed-bean-name>
 		<managed-bean-class>org.openuss.documents.FileInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope>
 	</managed-bean>

