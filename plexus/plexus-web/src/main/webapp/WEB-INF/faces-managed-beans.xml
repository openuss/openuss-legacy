 		<managed-bean-class>org.openuss.web.documents.FolderEntrySelection</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope>
 	</managed-bean>

 	<managed-bean>
 		<managed-bean-name>discussion_discussionentry</managed-bean-name>
 		<managed-bean-class>org.openuss.discussion.PostInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope>
 	</managed-bean>
 	
 	<managed-bean>
 		<managed-bean-name>discussion_topic</managed-bean-name>
 		<managed-bean-class>org.openuss.discussion.TopicInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope>
 	</managed-bean>
 	
 	<managed-bean>
 		<managed-bean-name>braincontest_answer</managed-bean-name>
 		<managed-bean-class>org.openuss.web.braincontest.AnswerWebInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope> 	
 	</managed-bean>

 	<managed-bean>
 		<managed-bean-name>braincontest_contest</managed-bean-name>
 		<managed-bean-class>org.openuss.braincontest.BrainContestInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope> 	
 	</managed-bean>

 	<managed-bean>
 		<managed-bean-name>news_selected_newsitem</managed-bean-name>
 		<managed-bean-class>org.openuss.news.NewsItemInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope> 	
 	</managed-bean>
 	
 	<managed-bean>
 		<managed-bean-name>newsletter_newsletter</managed-bean-name>
 		<managed-bean-class>org.openuss.newsletter.NewsletterInfo</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope> 	 		
 	</managed-bean>
 	
 	<managed-bean>
 		<managed-bean-name>newsletter_mail</managed-bean-name>
 		<managed-bean-class>org.openuss.newsletter.MailDetail</managed-bean-class>
 		<managed-bean-scope>session</managed-bean-scope> 	 		
 	</managed-bean>
 	
 	<managed-bean>
        <managed-bean-name>papersubmission_submission_selection</managed-bean-name>
        <managed-bean-class>org.openuss.web.papersubmission.SubmissionSelection</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    
    <managed-bean>
        <managed-bean-name>collaboration_current_folder</managed-bean-name>
        <managed-bean-class>org.openuss.documents.FolderInfo</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>collaboration_selected_folder</managed-bean-name>
        <managed-bean-class>org.openuss.documents.FolderInfo</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>collaboration_selected_file</managed-bean-name>
        <managed-bean-class>org.openuss.documents.FileInfo</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>collaboration_entry_selection</managed-bean-name>
        <managed-bean-class>org.openuss.web.documents.FolderEntrySelection</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>collaboration_member_selection</managed-bean-name>
        <managed-bean-class>org.openuss.web.collaboration.WorkspaceMemberSelection</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>    
    
    <managed-bean>
        <managed-bean-name>workspace</managed-bean-name>
        <managed-bean-class>org.openuss.collaboration.WorkspaceInfo</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>workspaceInfo</managed-bean-name>
        <managed-bean-class>org.openuss.collaboration.WorkspaceInfo</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <!--  Wiki -->   
    <managed-bean>
        <managed-bean-name>WikiSiteVersionInfo</managed-bean-name>
        <managed-bean-class>org.openuss.wiki.WikiSiteVersionInfo</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
	
	<!-- LDAP Authentication -->
	
	<managed-bean>
        <managed-bean-name>roleAttributeKeyInfo</managed-bean-name>
        <managed-bean-class>org.openuss.security.ldap.RoleAttributeKeyInfo</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
	
	<managed-bean>
        <managed-bean-name>attributeMappingInfo</managed-bean-name>
        <managed-bean-class>org.openuss.security.ldap.AttributeMappingInfo</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>authenticationDomainInfo</managed-bean-name>
        <managed-bean-class>org.openuss.security.ldap.AuthenticationDomainInfo</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>userDnPatternInfo</managed-bean-name>
        <managed-bean-class>org.openuss.security.ldap.UserDnPatternInfo</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>ldapServerInfo</managed-bean-name>
        <managed-bean-class>org.openuss.security.ldap.LdapServerInfo</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>


</faces-config>