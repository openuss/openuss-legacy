    - - - - - - - - - - - - - - - - - - - - - - - - - -->	
    
    <navigation-rule>
    	<from-view-id>/views/secured/papersubmission/examlist.xhtml</from-view-id>
    	<navigation-case>
    		<from-outcome>success</from-outcome>
    		<to-view-id>/views/secured/papersubmission/examlist.xhtml</to-view-id>
    	</navigation-case>
    	<navigation-case>
    		<from-outcome>failed</from-outcome>
    		<to-view-id>/views/secured/papersubmission/examlist.xhtml</to-view-id>
    	</navigation-case>
    </navigation-rule>
    
	<!-- - - - - - - - - - - - - - - - - - - - - - - - - -  
             Welcome-Page 
    - - - - - - - - - - - - - - - - - - - - - - - - - -->	
    <navigation-rule>
		<from-view-id>/views/welcome.xhtml</from-view-id>	
		
		<navigation-case>
			<from-outcome>university_registration_start</from-outcome>
			<to-view-id>/views/secured/lecture/registration/universitystart.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>university_registration_step1</from-outcome>		
			<to-view-id>/views/secured/lecture/registration/universitystep1.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>institute_registration_start</from-outcome>
			<to-view-id>/views/secured/lecture/registration/start.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>institute_registration_step1</from-outcome>		
			<to-view-id>/views/secured/lecture/registration/step1.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
		
</faces-config>

