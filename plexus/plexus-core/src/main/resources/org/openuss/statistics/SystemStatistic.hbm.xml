<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
    <class name="org.openuss.statistics.SystemStatisticImpl" table="SYSTEM_STATISTIC" dynamic-insert="false" dynamic-update="false">
        <cache usage="read-write" />
        <id name="id" type="java.lang.Long" unsaved-value="null">
            <column name="ID" sql-type="BIGINT"/>
            <generator class="sequence">
                <param name="sequence">GLOBAL_SEQUENCE</param>
            </generator>
        </id>
        <property name="users" type="java.lang.Long">
            <column name="USERS" not-null="true" unique="false" sql-type="BIGINT"/>
        </property>
        <property name="universities" type="java.lang.Long">
            <column name="UNIVERSITIES" not-null="true" unique="false" sql-type="BIGINT"/>
        </property>
        <property name="departments" type="java.lang.Long">
            <column name="DEPARTMENTS" not-null="true" unique="false" sql-type="BIGINT"/>
        </property>
        <property name="institutes" type="java.lang.Long">
            <column name="INSTITUTES" not-null="true" unique="false" sql-type="BIGINT"/>
        </property>
        <property name="courses" type="java.lang.Long">
            <column name="COURSES" not-null="true" unique="false" sql-type="BIGINT"/>
        </property>
        <property name="documents" type="java.lang.Long">
            <column name="DOCUMENTS" not-null="true" unique="false" sql-type="BIGINT"/>
        </property>
        <property name="posts" type="java.lang.Long">
            <column name="POSTS" not-null="true" unique="false" sql-type="BIGINT"/>
        </property>
        <property name="createTime" type="java.util.Date">
            <column name="CREATE_TIME" not-null="true" unique="false" sql-type="TIMESTAMP" index="createTime_index"/>
        </property>
    </class>
</hibernate-mapping>