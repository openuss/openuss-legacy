<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
    <class name="org.openuss.documents.FolderEntryImpl" table="DOCUMENTS_FOLDERENTRY" dynamic-insert="false" dynamic-update="false">
        <cache usage="read-write" />
        <id name="id" type="java.lang.Long" unsaved-value="null">
            <column name="ID" sql-type="BIGINT"/>
            <generator class="sequence">
                <param name="sequence">GLOBAL_SEQUENCE</param>
            </generator>
        </id>
        <property name="name" type="java.lang.String">
            <column name="NAME" not-null="false" unique="false" sql-type="VARCHAR(256)"/>
        </property>
        <property name="created" type="java.util.Date">
            <column name="CREATED" not-null="true" unique="false" sql-type="TIMESTAMP"/>
        </property>
        <property name="description" type="java.lang.String">
            <column name="DESCRIPTION" not-null="false" unique="false" sql-type="VARCHAR(1000)"/>
        </property>
        <many-to-one name="parent" class="org.openuss.documents.FolderImpl" foreign-key="DOCUMENTS_FOLDERENTRY_PARENT_C" lazy="proxy" fetch="select">
            <column name="PARENT_FK" not-null="false" sql-type="BIGINT"/>
        </many-to-one>
        <joined-subclass name="org.openuss.documents.FolderImpl" table="DOCUMENTS_FOLDER" dynamic-insert="false" dynamic-update="false" abstract="false">
            <key foreign-key="DOCUMENTS_FOLDERIFKC">
                <column name="ID" sql-type="BIGINT"/>
            </key>
            <property name="domainIdentifier" type="java.lang.Long">
                <column name="DOMAIN_IDENTIFIER" not-null="false" unique="true" sql-type="BIGINT"/>
            </property>
            <bag name="entries" order-by="PARENT_FK" lazy="true" fetch="select" inverse="true">
                <cache usage="read-write" />
                <key foreign-key="DOCUMENTS_FOLDERENTRY_PARENT_C">
                    <column name="PARENT_FK" sql-type="BIGINT"/>
                </key>
                <one-to-many class="org.openuss.documents.FolderEntryImpl"/>
            </bag>
        </joined-subclass>
        <joined-subclass name="org.openuss.documents.FileEntryImpl" table="DOCUMENTS_FILEENTRY" dynamic-insert="false" dynamic-update="false" abstract="false">
            <key foreign-key="DOCUMENTS_FILEENTRYIFKC">
                <column name="ID" sql-type="BIGINT"/>
            </key>
            <property name="contentType" type="java.lang.String">
                <column name="CONTENT_TYPE" not-null="true" unique="false" sql-type="VARCHAR(64)"/>
            </property>
            <property name="fileName" type="java.lang.String">
                <column name="FILE_NAME" not-null="true" unique="false" sql-type="VARCHAR(256)"/>
            </property>
            <property name="fileSize" type="java.lang.Integer">
                <column name="FILE_SIZE" not-null="true" unique="false" sql-type="INTEGER"/>
            </property>
            <property name="modified" type="java.util.Date">
                <column name="MODIFIED" not-null="true" unique="false" sql-type="TIMESTAMP"/>
            </property>
        </joined-subclass>
    </class>
</hibernate-mapping>