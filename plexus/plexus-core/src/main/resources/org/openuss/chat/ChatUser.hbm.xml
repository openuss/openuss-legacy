<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
    <class name="org.openuss.chat.ChatUserImpl" table="CHAT_USER" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="java.lang.Long" unsaved-value="null">
            <column name="ID" sql-type="BIGINT"/>
            <generator class="assigned">
            </generator>
        </id>
        <property name="displayName" type="java.lang.String">
            <column name="DISPLAY_NAME" not-null="true" unique="false" sql-type="VARCHAR(64)"/>
        </property>
        <property name="email" type="java.lang.String">
            <column name="EMAIL" not-null="true" unique="false" sql-type="VARCHAR(64)"/>
        </property>
    </class>
</hibernate-mapping>