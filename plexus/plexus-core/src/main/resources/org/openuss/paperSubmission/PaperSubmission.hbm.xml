<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="none">
    <class name="org.openuss.paperSubmission.PaperSubmissionImpl" table="PAPERSUBMISSION_PAPER" dynamic-insert="false" dynamic-update="false">
        <cache usage="read-write" />
        <id name="id" type="java.lang.Long" unsaved-value="null">
            <column name="ID" sql-type="BIGINT"/>
            <generator class="sequence">
                <param name="sequence">GLOBAL_SEQUENCE</param>
            </generator>
        </id>
        <property name="deliverDate" type="java.util.Date">
            <column name="DELIVER_DATE" not-null="true" unique="false" sql-type="TIMESTAMP"/>
        </property>
        <property name="comment" type="org.andromda.persistence.hibernate.usertypes.HibernateStringClobType">
            <column name="COMMENT" not-null="false" unique="false" sql-type="BLOB"/>
        </property>
        <many-to-one name="exam" class="org.openuss.paperSubmission.ExamImpl" cascade="none" foreign-key="PAPERSUBMISSION_PAPER_EXAM_FKC" lazy="proxy" fetch="select">
            <column name="EXAM_FK" not-null="false" sql-type="BIGINT"/>
        </many-to-one>
        <many-to-one name="sender" class="org.openuss.security.UserImpl" foreign-key="PAPERSUBMISSION_PAPER_SENDER_C" lazy="proxy" fetch="select">
            <column name="SENDER_FK" not-null="false" sql-type="BIGINT"/>
        </many-to-one>
    </class>
</hibernate-mapping>