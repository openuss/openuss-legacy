<project name="openuss :: foundation :: web :: build" default="install" basedir=".">
	<target name="compile">
		<exec executable="mvn.bat">
			<arg line="compile"/>
		</exec>
	</target>
	<target name="clean">
		<exec executable="mvn.bat">
			<arg line="clean"/>
		</exec>
	</target>
	<target name="install">
		<exec executable="mvn.bat">
			<arg line="install"/>
		</exec>
	</target>
	<target name="eclipse">
		<exec executable="mvn.bat">
			<arg line="eclipse:eclipse -DdownloadSources=true -Dwtpversion=1.0"/>
		</exec>
	</target>
	<target name="tomcat start">
		<exec executable="mvn.bat">
			<arg line="-e cargo:start"/>
		</exec>
	</target>
</project>